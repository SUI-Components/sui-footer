{
  "name": "@schibstedspain/sui-links",
  "version": "1.2.3",
  "main": "dist/",
  "keywords": [
    "SUI-Components",
    "ReactJS",
    "links",
    "link-list",
    "schibstedspain"
  ],
  "scripts": {
    "clean:dist": "rm -Rf ./dist/*",
    "predist": "npm run clean:dist -s",
    "dist": "npm run dist:scripts && npm run dist:styles",
    "dist:scripts": "babel src --out-dir dist",
    "dist:styles": "node-sass src/index.scss dist/_sui-links.scss --importer ./scripts/importer.js",
    "prepublish": "npm run dist",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:eslint": "eslint --ext=.js --ext=.jsx ./src/ ./test/ ./docs/",
    "lint:sass": "node_modules/.bin/sass-lint src/**/*.scss -c -v",
    "test": "mocha --compilers js:babel/register --recursive",
    "test:watch": "npm run test -- -w test src --watch-extensions jsx",
    "dev": "npm run dev:open && npm run dev:serve",
    "dev:serve": "webpack-dev-server --devtool eval --progress --colors --content-base docs/",
    "dev:open": "opener http://localhost:8080",
    "predoc": "webpack --config webpack.doc.config.js",
    "doc": "npm run doc:build",
    "doc:build": "gh-pages -d docs",
    "lint:js": "node_modules/.bin/eslint --ext .js,.jsx ./"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SUI-Components/sui-links.git"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "peerDependencies": {
    "classnames": "2",
    "react-router": "1 || 2"
  },
  "dependencies": {
    "@schibstedspain/theme-basic": "5"
  },
  "devDependencies": {
    "@schibstedspain/frontend-pre-commit-rules": "7",
    "@schibstedspain/sui-svgiconset": "1.0",
    "babel": "5.8.29",
    "babel-core": "5.8.30",
    "babel-loader": "5.3.2",
    "classnames": "2.2.0",
    "css-loader": "0.15.3",
    "expect": "1.6.0",
    "gh-pages": "0.3.1",
    "lodash": "3.9.3",
    "mocha": "2.2.5",
    "node-libs-browser": "0.5.2",
    "node-sass": "3.4.1",
    "opener": "1.4.1",
    "react": "0.14",
    "react-addons-test-utils": "0.14",
    "react-dom": "0.14",
    "react-hot-loader": "1.2.7",
    "react-router": "1.0.0-rc3",
    "sass-loader": "1.0.2",
    "style-loader": "0.12.3",
    "webpack": "1.9.11",
    "webpack-dev-server": "1.9.0"
  }
}
